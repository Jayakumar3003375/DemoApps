@model IEnumerable<RegistrationModel>
@{
    ViewData["Title"] = "Registration Page";
}
<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-dark">Registration List</h2>
        </div>
        <div class="col-6 text-end">
            <a class="btn btn-success" asp-controller="Registration" asp-action="Create">
                <i class="bi bi-plus-circle-fill"></i>&nbsp;&nbsp;Create New
            </a>
        </div>
    </div>

    <br />
    <br />
    <table class="table table-bordered table-striped" style="width:100%" id="tblRegistrationData">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Address
                </th>
                <th>
                    City
                </th>
                <th>
                    Mobile No
                </th>
                <th>
                    Email Id
                </th>
                <th>
                    Type
                </th>
                <th>
                    Action
                </th>
                
            </tr>
        </thead>        
    </table>
</div>
@section Scripts{
    <script src="~/js/registration.js"></script>
}
